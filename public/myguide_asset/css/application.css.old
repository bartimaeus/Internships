/* CUSTOM MYGUIDE */
.flat_area #myguide ul li{
	color: white;
	border-bottom-width: 0px;
	padding: 0px;
	margin: 0px;
	display: list-item;
}
.flat_area #myguide ul li label, .flat_area #myguide ul li input{
	padding: 0px;
	margin: 0px;
}
.flat_area #myguide ul li label{
	margin-left: 10px;
}
.flat_area #myguide ul li{
	margin: 10px 0px;
}
.flat_area #myguide ul li input{
	position: relative;
}
select, input {
  width: auto;
}
.stg .edit {
  position: absolute;
  top: 5px;
  right: 0px;
  cursor: pointer;
}
.year img.add {
  width:26px;
  position: absolute;
  top: 5px;
  right: 15px;
  cursor: pointer;
}
/* Special Chosen Overides */
.flat_area #myguide ul.chzn-results li {
  color: #666;
  padding: 7px 7px 8px;
  margin: 0px;
}
.flat_area #myguide ul.chzn-results li.highlighted {
  color: white;
}
.flat_area #myguide ul.chzn-results li:first-child {
padding-top: 7px;
}

.stg label:hover {
  text-decoration: underline;
  cursor: pointer;
}

.description_dialog{
  display: none;
}
.edit_img{
  width: 15px;
}